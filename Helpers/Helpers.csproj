<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>zblesk.Helpers</AssemblyName>
    <RootNamespace>zblesk.Helpers</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>zblesk.helpers</PackageId>
    <Authors>zblesk</Authors>
    <Company />
    <Product>zblesk.helpers</Product>
    <Description>My collection oft-used helpers.</Description>
    <PackageProjectUrl>https://github.com/zblesk/helpers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zblesk/helpers</RepositoryUrl>
    <Version>1.2.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="flurl" Version="3.0.2" />
    <PackageReference Include="flurl.http" Version="3.2.0" />
    <PackageReference Include="markdig" Version="0.26.0" />
  </ItemGroup>

</Project>
